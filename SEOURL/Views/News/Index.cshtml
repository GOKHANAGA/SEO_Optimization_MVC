@using SEOURL.Models
@model List<New>

@{
    ViewBag.Title = "Index";
}

<div class="container">
    <div class="row well">
        <h1>News</h1>
    </div>
    <div class="row">
        <table class="table-striped">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Title</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < Model.Count; i++)
                {
                    <tr>
                        <td>@Model[i].ID</td>
                        <td>@Model[i].Title</td>
                        <td><a href="/lastNews/@Model[i].SeoUrl">Details</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

